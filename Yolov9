      "cell_type": "code",
      "source": [
        "import os\n",
        "import zipfile\n",
        "\n",
        "# Set locale to UTF-8\n",
        "os.environ['LANG'] = 'en_US.UTF-8'\n",
        "os.environ['LC_ALL'] = 'en_US.UTF-8'\n",
        "\n",
        "# Define the folder to zip and the output zip file name\n",
        "folder_to_zip = '/content/runs'\n",
        "zip_file_name = '/content/yolov9_4900.zip'\n",
        "\n",
        "# Create a zip file\n",
        "with zipfile.ZipFile(zip_file_name, 'w', zipfile.ZIP_DEFLATED) as zipf:\n",
        "    for root, dirs, files in os.walk(folder_to_zip):\n",
        "        for file in files:\n",
        "            file_path = os.path.join(root, file)\n",
        "            zipf.write(file_path, os.path.relpath(file_path, folder_to_zip))\n",
        "\n",
        "print(f'Zipped {folder_to_zip} into {zip_file_name}')\n"
      ],
      "metadata": {
